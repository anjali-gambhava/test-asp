                        $("#divMenuToggle").addClass('-mr-2sm');
                        $("#divMenuToggle").removeClass('-mr-thin');
                        $('#divMenuToggle').find('polyline').attr('points', '9 18 15 12 9 6');
                    }
                });

                if ($('#navMenu').is(":visible")) {
                    $('#navMenu').slideLeft(function () {

                    });
                }
            });
            $("#btnUser").click(function () {
                $("#divUserMenu").toggleClass("d-hidden");
                return false;
            });
            $(window).on("click", function (e) {
                if (e.target.id !== "btnUser") {
                    $("#divUserMenu").addClass("d-hidden");
                }
            });
            function Logout() {
                window.location.href = 'Logout.aspx';
            }
            function ToggleSubMenu(cls) {
                var hascls = $('.' + cls).next('li').hasClass('d-hidden');
                $('li.submenu').addClass("d-hidden");
                $("li.submenu").find('polyline').attr('points', '6 9 12 15 18 9');
                //$('.' + cls).next('li').toggleClass("d-hidden");
                if (hascls) {
                    $('.' + cls).next('li').removeClass("d-hidden");
                    $('.' + cls).find('polyline').last().attr('points', '18 15 12 9 6 15');
                }
                else {
                    $('.' + cls).next('li').addClass("d-hidden");
                    $('.' + cls).find('polyline').last().attr('points', '6 9 12 15 18 9');
                }
            }
